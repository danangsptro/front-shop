"card border-0 rounded shadow\">\r\n                    <div class=\"card-body\">\r\n                        <h5 class=\"font-weight-bold\"><i class=\"fas fa-shopping-cart\"></i>MY ORDER</h5>\r\n                        <hr>\r\n                        <table class=\"table table-striped table-bordered\">\r\n                            <thead class=\"thead-dark\">\r\n                                <tr>\r\n                                    <th scope=\"col\">INVOICE</th>\r\n                                    <th scope=\"col\">FULL NAME</th>\r\n                                    <th scope=\"col\">SHIPPING</th>\r\n                                    <th scope=\"col\">GRAND TOTAL</th>\r\n                                    <th scope=\"col\">OPTIONS</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <tr v-for=\"order in orders\" :key=\"order.id\">\r\n                                    <th>{{ order.invoice }}</th>\r\n                                    <td>{{ order.name }}</td>\r\n                                    <td>{{ order.courier.toUpperCase() }}</td>\r\n                                    <td>rp. {{ moneyFormat(order.grand_total) }}</td>\r\n                                    <td class=\"text-center\">\r\n                                        <router-link :to=\"{name: 'detail_order', params:{snap_token}}\" class=\"btn btn-sm btn-primary\">DETAIL</router-link>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport CustomerMenu from '@/components/CustomerMenu'\r\nimport {computed, onMounted} from 'vue'\r\nimport {useStore} from 'vuex'\r\nexport default {\r\n    components: {\r\n        CustomerMenu\r\n    },\r\n    setup() {\r\n        const store = useStore()\r\n        onMounted(() => {\r\n            return store.dispatch('order/getOrder')\r\n        })\r\n\r\n        const orders = computed(() => {\r\n            return store.getters['order/getOrder']\r\n        })\r\n\r\n        return {\r\n            store,\r\n            orders\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .table.thead-dark th {\r\n        color: #fff;\r\n        background-color: #6777ef;\r\n        border-color: #6777ef;\r\n    }\r\n</style>"]}]}