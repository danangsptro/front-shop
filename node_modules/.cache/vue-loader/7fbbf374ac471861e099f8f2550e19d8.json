an>\r\n                            </router-link>\r\n                        </div>\r\n                        <div class=\"col-md-5 d-none d-md-block\">\r\n                            <form class=\"searc-wrap\">\r\n                                <div class=\"input-group w-100\">\r\n                                    <input type=\"text\" class=\"form-control search-form\" style=\"width:55%;border 1px solid #ffffff\" name=\"q\" placeholder=\"mau beli apa hari ini ?\">\r\n                                    <div class=\"input-group-append\">\r\n                                        <button class=\"btn search-button\" type=\"submit\"><i class=\"fa fa-search\"></i></button>\r\n                                    </div>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                        <div class=\"col-md-4 col-5\">\r\n                            <div class=\"d-flex justify-content-end\">\r\n                                <div class=\"cart-header\">\r\n                                    <a href=\"#\" class=\"btn search-button btn-md\" style=\"color:#ffffff; background-color:#6677ef; border-color:#ffffff;\"><i class=\"fa fa-shopping-cart\"></i>{{cartCount}}  | Rp. {{ moneyFormat(cartTotal) }}</a>\r\n                                </div>\r\n                                <div class=\"account\">\r\n                                    <router-link :to=\"{name: 'login'}\" class=\"btn search-button btn-md d-none d-md-block ml-4\"><i class=\"fa fa-user-circle\"></i>ACCOUNT</router-link>\r\n                                    <router-link :to=\"{name: 'dashboard'}\" class=\"btn search-button btn-md d-none d-md-block ml-4\"><i class=\"fa fa-tachometer-alt\"></i>DASHBOARD</router-link>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </section>\r\n        </header>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted} from 'vue'\r\nimport {useStore} from 'vuex'\r\nexport default {\r\n    setup() {\r\n        const store = useStore()\r\n\r\n        const isLoggedIn = computed(() => {\r\n            return store.getters['auth/isLoggedIn']\r\n        })\r\n\r\n        const cartCount = computed(() => {\r\n            return store.getters['cart/cartCount']\r\n        })\r\n        \r\n        const cartTotal = computed(() => {\r\n            return  store.getters['cart/cartTotal']\r\n        })\r\n\r\n        onMounted(() => {\r\n            const token = store.state.auth.token\r\n            if (!token) {\r\n                return\r\n            }\r\n\r\n            store.dispatch('cart/cartCount')\r\n            store.dispatch('cart/cartTotal')\r\n        })\r\n\r\n        return {\r\n            store,\r\n            isLoggedIn,\r\n            cartTotal,\r\n            cartCount\r\n        }\r\n    }\r\n}\r\n</script>"]}]}