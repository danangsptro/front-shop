,IARL,EAQK,CAPD,WAOC,EAND,WAMC,EALD,mBAAA,CAIK,IAJL,EAIK,IAJL,EAIK,CAHD,mBAAA,CAEY,MAFZ,EAAA,WAAA,EAEY,gBAAA,CADL,MAAA,CAAA,OAAA,CAAQ,MACH,CAAA,GADY,OADxB,EAEK;AAAA;AAFL,GAGC,CAJL,CAKC,CARL,CASI,CArBR,CAsBI,CAvBR,CAyBE,EADF,mBAAA,CAAmM,QAAnM,EAAmM;AAA1L,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA,UAAA,MAAA;AAAA,aAAU,MAAA,CAAA,SAAA,CAAU,MAAA,CAAA,OAAA,CAAQ,EAAlB,EAAsB,IAAA,CAAA,iBAAA,CAAkB,MAAA,CAAA,OAAlB,CAAtB,EAAkD,MAAA,CAAA,OAAA,CAAQ,MAA1D,CAAV;AAAA,KAAA,EAA0E,CAAA,SAAA,CAA1E,CAAA,CAAqL;AAAxG,IAAA,KAAK,EAAC;AAAkG,GAAnM,E,WAAA,CACE,CArCN,CAsCE,CAvCN,CAwCE,CAzCN,CA0CE,CAlDN,CA8DE,EAXF,mBAAA,CAUM,KAVN,EAAA,WAAA,EAUM,CATF,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,CAPF,mBAAA,CAMM,KANN,EAAA,WAAA,EAMM,CALF,mBAAA,CAIM,KAJN,EAAA,WAAA,EAIM,CAHF,WAGE,EAFF,WAEE,EADF,mBAAA,CAAoC,KAApC,EAAoC;AAA/B,IAAA,SAAwB,EAAhB,MAAA,CAAA,OAAA,CAAQ;AAAe,GAApC,E,IAAA,E;;AAAA,I,WAAA,CACE,CAJN,CAKE,CANN,CAOE,CARN,CASE,CAVN,CAWE,CA/DN,C","sourcesContent":["<template>\r\n    <div class=\"container-fluid\" mt-5 mb-5>\r\n        <div class=\"row\">\r\n            <div class=\"col-md-4\">\r\n                <div class=\"card border-0 rounded shadow\">\r\n                    <div class=\"card-body p-2\">\r\n                        <img :src=\"product.image\" class=\"w-100 border\">\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"col-md-8\">\r\n                <div class=\"card border-0 shadow rounded\">\r\n                    <div class=\"card-body\">\r\n                        <label class=\"font-weight-bold\" style=\"font-size: 20px\"> {{ product.title }}</label>\r\n                        <hr>\r\n                        <div class=\"price-product\" id=\"price-product\" style=\"font-size: 1.35rem\">\r\n                            <span class=\"font-weight-bold mr-4\" style=\"color:green\">\r\n                                Rp. {{ calculateDiscount(product) }}\r\n                            </span>\r\n                            <s class=\"font-weight-bold\" style=\"text-decoration-color:red\">\r\n                                Rp. {{ product.price }}\r\n                            </s>\r\n                        </div>\r\n\r\n                        <table class=\"table table-borderless mt-3\">\r\n                            <tbody>\r\n                                <tr>\r\n                                    <td class=\"font-weight-bold\">\r\n                                        diskon\r\n                                    </td>\r\n                                    <td>:</td>\r\n                                    <td>\r\n                                        <button class=\"btn btn-sm\" style=\"color:#ff2f00;border-color:#ff2f00\">\r\n                                            Diskon {{ product.discount }}\r\n                                        </button>\r\n                                    </td>\r\n                                </tr>\r\n                                <tr>\r\n                                    <td class=\"font-weight-bold\">Berat</td>\r\n                                    <td>:</td>\r\n                                    <td>\r\n                                        <span class=\"bage badge-pill badge-success\" style=\"font-size: 14px; border-radius: .3rem; padding: .25em .5em .2em\">\r\n                                            {{ product.weight }}    \r\n                                         gram</span>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                        <button @click.prevent=\"addToCart(product.id, calculateDiscount(product), product.weight)\" class=\"btn btn-primary btn-lg btn-block\"><i class=\"fa fa-shopping-cart\"></i>Tambah ke keranjang</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"row mt-5\">\r\n            <div class=\"col-md-12\">\r\n                <div class=\"card border-0 rounded shadow\">\r\n                    <div class=\"card-body\">\r\n                        <label class=\"font-weight-bold\" style=\"font-size: 20px\">KETERANGAN</label>\r\n                        <hr>\r\n                        <div v-html=\"product.content\"></div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted} from 'vue'\r\nimport {useStore} from 'vuex'\r\nimport {useRoute, useRouter} from 'vue-router'\r\nexport default {\r\n    setup() {\r\n        const store = useStore()\r\n        const router = useRouter()\r\n        const route = useRoute()\r\n        onMounted(() => {\r\n            store.dispatch('category/getCategories')\r\n        })\r\n\r\n        const product = computed(() => {\r\n            return store.state.product.product\r\n        })\r\n\r\n        function addToCart(product_id, price, weight){\r\n            const token = store.state.auth.token\r\n\r\n            if (!token) {\r\n                return router.push({name: 'login'})\r\n            }\r\n\r\n            store.dispatch('cart/addToCart', {\r\n                product_id: product_id,\r\n                price: price,\r\n                weight: weight,\r\n                quantity: 1\r\n            })\r\n        }\r\n\r\n        return {\r\n            store,\r\n            product,\r\n            router,\r\n            route,\r\n            addToCart\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}